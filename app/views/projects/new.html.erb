<h1>Add a project</h1>

<%= form_for @project do |f| %>

      <div class="row">
        <div class="input-field col s12">
          <%= f.label :name %>
          <%= f.text_field :name, class: 'validate' %>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s12">
          <%= f.label :description %>
          <%= f.text_field :description, class: 'validate' %>
        </div>
      </div>

  <%= f.button %>
<% end %>

